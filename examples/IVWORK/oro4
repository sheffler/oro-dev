#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x19b9fc0 .scope module, "top" "top" 2 2;
 .timescale 0 0;
L_0x19f2a30 .functor AND 1, L_0x19f2b20, L_0x19f2c60, C4<1>, C4<1>;
v0x19b9c40_0 .net *"_s6", 0 0, L_0x19f2b20; 1 drivers
v0x19f2320_0 .net *"_s8", 0 0, L_0x19f2c60; 1 drivers
v0x19f23c0_0 .net "andres", 0 0, L_0x19f2a30; 1 drivers
v0x19f2460_0 .net "clk", 31 0, v0x19f2510_0; 1 drivers
v0x19f2510_0 .var "clk_d", 31 0;
v0x19f25b0_0 .var/i "i", 31 0;
v0x19f2690_0 .net "oro", 31 0, v0x19f2730_0; 1 drivers
v0x19f2730_0 .var "oro_d", 31 0;
v0x19f2820_0 .var "res", 31 0;
E_0x19b9e70 .event edge, v0x19f2690_0;
E_0x19bccd0 .event edge, v0x19f2460_0;
E_0x19ba0b0 .event edge, v0x19f23c0_0;
L_0x19f2b20 .part v0x19f2510_0, 0, 1;
L_0x19f2c60 .part v0x19f2730_0, 0, 1;
    .scope S_0x19b9fc0;
T_0 ;
    %set/v v0x19f25b0_0, 0, 32;
T_0.0 ;
    %load/v 8, v0x19f25b0_0, 32;
   %cmpi/s 8, 10, 32;
    %jmp/0xz T_0.1, 5;
    %delay 5, 0;
    %movi 8, 1, 32;
    %set/v v0x19f2510_0, 8, 32;
    %delay 5, 0;
    %set/v v0x19f2510_0, 0, 32;
    %delay 5, 0;
    %movi 8, 1, 32;
    %set/v v0x19f2510_0, 8, 32;
    %delay 5, 0;
    %set/v v0x19f2510_0, 0, 32;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x19f25b0_0, 32;
    %set/v v0x19f25b0_0, 8, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .thread T_0;
    .scope S_0x19b9fc0;
T_1 ;
    %wait E_0x19ba0b0;
    %vpi_call 2 29 "$display", "VERILOG %t SAW ANDRES CHANGE: %d\012", $time, v0x19f23c0_0;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x19b9fc0;
T_2 ;
    %wait E_0x19bccd0;
    %vpi_call 2 34 "$display", "VERILOG %t SAW CLK CHANGE: %d\012", $time, v0x19f2460_0;
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x19b9fc0;
T_3 ;
    %wait E_0x19b9e70;
    %vpi_call 2 39 "$display", "VERILOG %t SAW ORO CHANGE: %d\012", $time, v0x19f2690_0;
    %jmp T_3;
    .thread T_3, $push;
    .scope S_0x19b9fc0;
T_4 ;
    %set/v v0x19f2510_0, 0, 32;
    %set/v v0x19f2730_0, 0, 32;
    %vpi_call 2 49 "$display", "VERILOG DISPLAY AT TIME 0\012";
    %vpi_call 2 50 "$dumpvars";
    %vpi_func 2 52 "$apvm", 8, 32, "oro", "oroboro", "oroboro";
    %set/v v0x19f2820_0, 8, 32;
    %vpi_call 2 53 "$display", "VERILOG %t APVM returns!\012", $time;
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "oro4.v";
